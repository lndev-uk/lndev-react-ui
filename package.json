{
  "name": "@lndev/lndev-react-ui",
  "version": "2.1.0",
  "description": "Minimal UI strater kit",
  "author": "Luis Nesi",
  "license": "MIT",
  "repository": "git@github.com:lndev/lndev-react-ui.git",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "homepage": "https://lndev.github.io/lndev-react-ui",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "npm run lndev && cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "npm run lndev && react-scripts test --env=jsdom",
    "build": "npm run lndev && rollup -c && cp -r ./src/scss ./dist && npm run scss:compile",
    "build:css": "npm run lndev && npm run webpack:styles && npm run webpack:styles-editor && npm run webpack:clean",
    "build:storybook": "npm run lndev && build-storybook -o ./build/",
    "start": "npm run lndev && rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "npm run lndev && npm run build:storybook",
    "deploy": "gh-pages -d ./build",
    "storybook": "npm run lndev && start-storybook -p 9009",
    "scss:compile": "npm run webpack:styles && npm run webpack:styles-editor",
    "webpack:styles": "webpack --config=config/webpack.styles.js",
    "webpack:styles-editor": "webpack --config=config/webpack.styles-editor.js",
    "webpack:clean": "rm dist/css/main.js && rm dist/css/main.js.map",
    "lndev": "node LNDev.js",
    "react-docgen": "react-docgen"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@storybook/addon-actions": "^5.3.8",
    "@storybook/addon-docs": "^5.3.8",
    "@storybook/addon-links": "^5.3.8",
    "@storybook/addon-viewport": "^5.3.8",
    "@storybook/addons": "^5.3.8",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.8",
    "@storybook/theming": "^5.3.8",
    "@svgr/rollup": "^2.4.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "animate-sass": "^0.8.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^4.4.1",
    "cross-env": "^5.1.4",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^3.1.0",
    "flow-bin": "^0.113.0",
    "gh-pages": "^1.2.0",
    "mdx-loader": "^3.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.12",
    "react-docgen": "^5.1.0",
    "react-dom": "^16.12",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^16.12.0",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0",
    "sass-loader": "^8.0.0",
    "storybook": "^5.3.8",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "styled-components": "^4.4.1"
  }
}
